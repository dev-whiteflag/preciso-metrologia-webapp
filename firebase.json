{
  "hosting": {
    "public": "dist/preciso-app/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/login",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/home/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/home",
        "destination": "/",
        "type": 301
      }
    ]
  }
}